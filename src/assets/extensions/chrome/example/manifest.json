{
  "manifest_version": 3,
  "name": "Moss Example Extension",
  "version": "1.0.0",
  "description": "A minimal example extension for the Moss antidetect browser.",
  "permissions": [
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content_script.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "action": {
    "default_title": "Moss Example"
  }
}
